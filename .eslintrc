{
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb/base",
    "react-app",
    "prettier"
  ],
  "plugins": [
    "only-warn",
    "prettier",
    "import",
    "react-hooks",
    "jest"
  ],
  "rules": {
    "prettier/prettier": "warn",
    "no-console": 0,
    "quotes": ["warn", "single"],
    "import/no-named-as-default": 0,
    "no-param-reassign": 0
  }
}
